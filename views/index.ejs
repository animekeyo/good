    <script>
        async function run(val, limit, type) {
            const apply = $('.flex-box-items');
            const data = await fetch('https://anime.animekeyo.com/?limit=' + limit + '&q=' + val).then(o => o.json());
            data.forEach(e => {
                // console.log(e.title)
                if ($('[id="' + e.id + '"]').length) {

                } else {
                    const theme = `
                       <div id="${e.id}" class="item">
                        <div class="cover">
                            <div class="poster" style="background-image:url(https://images.weserv.nl/?url=anime.animekeyo.com/cover/${e.cover}&w=50)"></div>
                        </div>
                        <div class="details">
                        <div class="title">
                        ${e.title}
                        </div>
                        <div class="episodes">
                             ${e.type} - Episodes ${e.episodes}
                        </div>
                        </div>
                       
                        </div>`;
                    apply.prepend(theme).show();
                }

            });
        }


        run('', 10, 'no')
        $('input').on('input', function() {
            const apply = $('.flex-box-items');
            if ($(this).val() === '') {
                apply.hide()
            } else {
                apply.hide();
                apply.empty();
                run($(this).val(), 5, 'no')
            }

        });
    </script>
